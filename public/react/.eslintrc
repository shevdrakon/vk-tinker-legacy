{
  "env": {
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "globals": {
    "require": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "plugins": [
    "react",
    "import"
  ],
//  "settings": {
//    "import/resolver": {
//      "webpack": {
//        "config": {
//          "resolve": {
//            "extensions": [
//              "",
//              ".js",
//              ".jsx"
//            ]
//          }
//        }
//      }
//    }
//  },
  "rules": {
    "import/no-named-as-default": 0
  }
}